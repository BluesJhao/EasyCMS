<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.easycms.model">
    <class name="Operlogs" table="operlogs">
        <id name="id" column="id" type="string">
            <generator class="uuid"/>
        </id>
        <property name="loginName" column="loginName" type="string"/>
        <property name="operTime" column="operTime" type="timestamp"/>
        <property name="content" column="content" type="string"/>
        <property name="ip" column="ip" type="string"/>
    </class>
</hibernate-mapping>