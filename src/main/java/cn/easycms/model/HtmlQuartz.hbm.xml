<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.easycms.model">
    <class name="HtmlQuartz" table="htmlquartz">
        <id name="id" type="string"><generator class="uuid"/></id>
        <property name="siteId" type="string"/>
        <property name="channelId" type="string"/>
        <property name="type" type="string"/>
        <property name="exeTimeHour" type="java.lang.Integer"/>
        <property name="exeTimeMin" type="java.lang.Integer"/>
        <property name="intervalType" type="string"/>
        <property name="intervalHour" type="java.lang.Integer"/>
        <property name="intervalMin" type="java.lang.Integer"/>
    </class>
</hibernate-mapping>