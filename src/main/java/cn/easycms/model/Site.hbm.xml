<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.easycms.model">
    <class name="Site" table="site">
        <id name="id" column="id" type="string">
            <generator class="uuid"></generator>
        </id>
        <property name="name" type="string"></property>
        <property name="siteDomain" type="string"></property>
        <property name="sourcePath" type="string"></property>
        <property name="copyright" type="string"></property>
        <property name="recordCode" type="string"></property>
        <property name="parId" type="string"></property>
        <property name="state" type="string"></property>
        <property name="url" type="string"></property>
        <property name="indexTemplate" type="string"></property>
        <property name="logo" type="string"></property>
        <property name="orderNum" type="java.lang.Integer"></property>
        <property name="clickNum" type="java.lang.Integer"></property>

    </class>
</hibernate-mapping>